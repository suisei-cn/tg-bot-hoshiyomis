{
  "name": "tg-hosymbot",
  "version": "2.0.0",
  "description": "Telegram bot for Hoshiyomis.",
  "main": "src/index.js",
  "scripts": {
    "launch": "npm run webhook disable && nodemon src/index.ts",
    "dev": "npm run launch",
    "webhook": "_ () { ts-node src/util/webhook.ts \"$*\"; }; _",
    "pub": "wrangler publish && npm run webhook enable",
    "test": "jest --watch",
    "coverage": "jest --collectCoverageFrom=src/**/*.ts --coverage",
    "build": "tsc",
    "build:watch": "tsc -w",
    "clear": "rimraf dist node_modules coverage worker",
    "postinstall": "patch-package"
  },
  "keywords": [
    "telegram",
    "bot",
    "telegraf",
    "typescript",
    "cloudflare",
    "workers"
  ],
  "author": "Outvi V <oss+npm@outv.im>",
  "license": "MIT",
  "dependencies": {
    "@cfworker/web": "^1.6.5",
    "cfworker-middware-telegraf": "^1.0.0",
    "telegraf": "^3.38.0",
    "typescript": "^4.1",
    "wanakana": "^4.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.1.0",
    "@types/jest": "^24.0.12",
    "@types/node-fetch": "^2.5.8",
    "jest": "^24.3.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^1.18.10",
    "patch-package": "^6.2.2",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^8.0.3",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "webpack": "^4.10.0",
    "webpack-cli": "^4.4.0"
  }
}
